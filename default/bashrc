# If not running interactively, don't do anything
[[ $- != *i* ]] && return

# Source default Omadora bash configuration if it exists
if [ -f ~/.local/share/omadora-kde/default/bash/rc ]; then
  source ~/.local/share/omadora-kde/default/bash/rc
fi

